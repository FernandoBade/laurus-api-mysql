{
  "name": "laurus-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "dev:sync": "ts-node src/utils/database/sync/dbSync.ts && nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "node --test",
    "dev:create": "ts-node src/dev/seedDevDatabase.ts",
    "dev:reset": "ts-node src/dev/resetDevDatabase.ts",
    "dev:analyze": "node -r ts-node/register -r dotenv/config ./src/dev/analyseUser.ts",
    "dev:list-users": "node -r ts-node/register -r dotenv/config ./src/dev/analyseUser.ts --list",
    "dev:db:diag": "node -r ts-node/register -r dotenv/config ./src/dev/dbDiag.ts",
    "dev:tx-stats": "node -r ts-node/register -r dotenv/config ./src/dev/txDiag.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.36.4",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "translation-check": "^1.1.0",
    "typescript": "^5.7.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "drizzle-kit": "^0.30.0",
    "eslint": "^9.19.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2"
  }
}
